<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#4682b4"
        tools:context=".ryge">

    <Button
            android:id="@+id/stressBtn"
            android:layout_width="172dp"
            android:layout_height="93dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="95dp"
            android:text="Stress"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
            android:id="@+id/alkoBtn"
            android:layout_width="172dp"
            android:layout_height="93dp"
            android:layout_marginTop="95dp"
            android:layout_marginEnd="10dp"
            android:text="Alkohol"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
            android:id="@+id/sociBtn"
            android:layout_width="172dp"
            android:layout_height="93dp"
            android:layout_marginTop="50dp"
            android:layout_marginEnd="10dp"
            android:text="Socialt"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/alkoBtn" />

    <Button
            android:id="@+id/udBtn"
            android:layout_width="172dp"
            android:layout_height="93dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="50dp"
            android:text="Udmattet "
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/stressBtn" />

    <TextView
            android:id="@+id/textView3"
            android:layout_width="293dp"
            android:layout_height="55dp"
            android:layout_marginStart="59dp"
            android:layout_marginTop="59dp"
            android:layout_marginEnd="59dp"
            android:text="Hvorfor har du rÃ¸get?"
            android:textColor="#ffffff"
            android:textSize="30sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>